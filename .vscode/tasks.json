{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build C++ file",
      "type": "shell",
      "command": "g++",
      "args": ["-g", "${workspaceFolder}/PRACTISE CODES C++/*.cpp", "-o", "${workspaceFolder}/PRACTISE CODES C++/${fileBasenameNoExtension}"],
